{
  "productName": "mailalytics-desktop",
  "version": "0.0.0",
  "identifier": "com.angelxmoreno.mailalytics",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "mailalytics-desktop",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/mailalytics-logo-32.png",
      "icons/mailalytics-logo-128.png",
      "icons/mailalytics-logo-128@2x.png",
      "icons/mailalytics-logo.icns",
      "icons/mailalytics-logo.ico"
    ]
  }
}
